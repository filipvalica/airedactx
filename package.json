{
  "name": "airedactx-extension",
  "version": "1.3.0",
  "description": "A browser extension for content modification and automation",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "rimraf dist",
    "build:ui": "vite build --config vite.config.ts",
    "build:background": "vite build --config vite.background.config.ts",
    "build:content": "vite build --config vite.content.config.ts",
    "build": "npm run build:ui && npm run build:background && npm run build:content",
    "build:firefox": "cross-env BROWSER=firefox npm run build",
    "build:chrome": "cross-env BROWSER=chrome npm run build",
    "preview": "vite preview"
  },
  "keywords": [
    "browser-extension",
    "content-modification",
    "automation"
  ],
  "author": "filipvalica",
  "license": "MIT",
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@types/node": "^24.6.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/webextension-polyfill": "^0.12.3",
    "@vitejs/plugin-react": "^5.0.4",
    "cross-env": "^10.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  }
}